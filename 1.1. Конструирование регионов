      "cell_type": "code",
      "source": [
        "# Функция редактирования некорректного поля region в нашем любимом датасете\n",
        "def override_gai_region(table, field):\n",
        "  right_gai_region = table.copy();\n",
        "  for i in range (0, len(right_gai_region)):\n",
        "    broken_id = right_gai_region.at[i, field];\n",
        "    right_id = gai_region[broken_id];\n",
        "    right_gai_region.at[i, field] = right_id;\n",
        "  return right_gai_region;\n",
        "\n",
        "\n",
        "\n",
        "# Редактирование поля genre\n",
        "right_apartments = override_gai_region(apartments, 'region');\n",
        "right_apartments.head()\n"
      ],
