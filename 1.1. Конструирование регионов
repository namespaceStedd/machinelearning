        "# Нормализация регионов\n",
        "def define_gai_region(table):\n",
        "  region_dictionary = {};\n",
        "  for i in range (0, len(table)):\n",
        "    right_id = table.at[i, 'id'];\n",
        "    broken_id = table.at[i, 'di'];\n",
        "    region_dictionary[broken_id] = right_id;\n",
        "  return region_dictionary;\n",
        "\n",
        "\n",
        "# Получение словаря\n",
        "gai_region = define_gai_region(region);\n",
        "gai_region"
